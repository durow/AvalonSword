<Window x:Class="WpfSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSample" WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="825">
    <DockPanel>
        <WrapPanel DockPanel.Dock="Top">
            <Button Content="AddText" Width="90" Command="{Binding CmdAddText}"/>
            <Button Content="OpenView" Width="90" Command="{Binding CmdOpenView}"/>
            <Button Content="RouterTest" Width="90" Command="{Binding Router}" CommandParameter="RouterTest"/>
            <Button Content="RouterTest2" Width="90" Command="{Binding Router}" CommandParameter="RouterTest2"/>
            <CheckBox IsChecked="{Binding CanRouterTest}" VerticalAlignment="Center" Content="CanRouterTest"/>
        </WrapPanel>
        <TextBox Text="{Binding ResultText}"/>
    </DockPanel>
</Window>
